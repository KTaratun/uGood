<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:padding="15dp"
        android:text="@string/example"
        android:textSize="20sp"/>


    <ImageView
        android:id="@+id/thumb_icon"
        android:contentDescription="@null"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="12dp"
        android:layout_marginStart="65dp"
        android:layout_alignParentStart="true"
        android:background="@drawable/proxy_duckduckgo_com"/>
    <TextView
        android:id="@+id/thumb_val"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="15dp"
        android:text="@string/example"
        android:layout_toEndOf="@id/thumb_icon"
        android:textSize="20sp"/>


    <ImageView
        android:id="@+id/grid_icon"
        android:contentDescription="@null"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="12dp"
        android:layout_marginStart="35dp"
        android:layout_toEndOf="@id/thumb_icon"
        android:background="@drawable/ic_grid_on_black_24dp"/>
    <TextView
        android:id="@+id/grid_val"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="15dp"
        android:text="@string/example"
        android:layout_toEndOf="@id/grid_icon"
        android:textSize="20sp"/>


    <ImageView
        android:id="@+id/search_icon"
        android:contentDescription="@null"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="12dp"
        android:layout_marginStart="35dp"
        android:layout_toEndOf="@id/grid_icon"
        android:background="@drawable/ic_search_black_24dp"/>
    <TextView
        android:id="@+id/search_val"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="15dp"
        android:text="@string/example"
        android:layout_toEndOf="@id/search_icon"
        android:textSize="20sp"/>

</RelativeLayout>